    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Main",
            "program": "${workspaceFolder}\\index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Scrape Options",
            "program": "${workspaceFolder}\\index.js",
            "args": [
                "--scrape-options",
                "--headless=false"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Db To Json --get-last",
            "program": "${workspaceFolder}\\index.js",
            "args": [
                "--db-to-json",
                "--get-last"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Db To Json scrapeId",
            "program": "${workspaceFolder}\\index.js",
            "args": [
                "--db-to-json",
                "--scrape-options=e8933d90-3c3f-11e8-b959-c7fc6e258c69"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Generate Input Range",
            "program": "${workspaceFolder}\\index.js",
            "args": [
                "--generate-input-range"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Scrape Quotes Last",
            "program": "${workspaceFolder}\\index.js",
            "args": [
                "--scrape-quotes",
                "--headless=false",
                "--get-last"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Scrape Quotes Id",
            "program": "${workspaceFolder}\\index.js",
            "args": [
                "--scrape-quotes",
                "--headless=false",
                "--batch-id=42571170-3dfe-11e8-b7c8-f3d0eaa9ee32"
            ]
        }
    ]
}   